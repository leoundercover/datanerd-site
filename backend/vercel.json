{
    "version": 2,
    "builds": [
        {
            "src": "server.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.py"
        }
    ],
    "env": {
        "MONGO_URL": "@mongo_url",
        "DB_NAME": "datanerd",
        "CORS_ORIGINS": "*"
    }
}